		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗЛИЧНЫЕ
	Задачи.Родитель КАК Родитель
ПОМЕСТИТЬ ВТ_Родители
ИЗ
	Справочник.Задачи КАК Задачи
ГДЕ
	Задачи.Родитель &lt;&gt; ЗНАЧЕНИЕ(Справочник.Задачи.ПустаяСсылка)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Задачи.Ссылка КАК Ссылка,
	Задачи.Наименование КАК Наименование,
	Задачи.Владелец КАК Владелец,
	Задачи.СтатусЗадачи КАК СтатусЗадачи,
	Задачи.ДатаНачала КАК ДатаНачала,
	Задачи.ДатаОкончания КАК ДатаОкончания
ИЗ
	Справочник.Задачи КАК Задачи
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Родители КАК ВТ_Родители
		ПО Задачи.Ссылка = ВТ_Родители.Родитель
ГДЕ
	ЕСТЬNULL(ВТ_Родители.Родитель, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО</query>
	</dataSet>
