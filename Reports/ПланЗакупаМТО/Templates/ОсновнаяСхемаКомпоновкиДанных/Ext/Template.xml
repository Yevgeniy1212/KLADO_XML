ИЗ
	Справочник.Договоры.МТО КАК ДоговорыМТО

СГРУППИРОВАТЬ ПО
	ДоговорыМТО.Ссылка

ИМЕЮЩИЕ
	КОЛИЧЕСТВО(ДоговорыМТО.Ссылка) &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДоговорыМТО.Сумма КАК Сумма,
	ДоговорыМТО.Цена КАК Цена,
	ДоговорыМТО.Количество КАК Количество,
	ДоговорыМТО.Материал КАК Материал,
	ДоговорыМТО.ДатаПотребности КАК ДатаПотребности,
	ДоговорыМТО.Ссылка КАК Ссылка
ИЗ
	ВТ_Договоры КАК ВТ_Договоры
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Договоры.МТО КАК ДоговорыМТО
		ПО ВТ_Договоры.Ссылка = ДоговорыМТО.Ссылка
ГДЕ
	ДоговорыМТО.Ссылка.ТипДоговора = &amp;ТипДоговора
{ГДЕ
	(ДоговорыМТО.Ссылка.ДатаОкончания &lt; &amp;ДатаДоговора) КАК ДатаДоговора,
	(ДоговорыМТО.Ссылка.ДатаНачала &gt; &amp;ДатаДоговора) КАК ДатаДоговора,
	(ДоговорыМТО.Ссылка.Контрагент = &amp;Контрагент) КАК Контрагент,
	(ДоговорыМТО.Ссылка.Ссылка = &amp;Договор) КАК Договор,
	(ДоговорыМТО.Ссылка.СостояниеИсполнения = &amp;Статус) КАК Статус}</query>
